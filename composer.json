{
    "name": "codefocus/managedcache",
    "type": "library",
    "description": "ManagedCache description",
    "keywords": [
        "codefocus",
        "managedcache",
        "cache",
        "laravel",
        "invalidation",
        "clear"
    ],
    "homepage": "https://github.com/codefocus/managedcache",
    "license": "MIT",
    "authors": [
        {
            "name": "Menno van Ens",
            "email": "info@codefocus.ca",
            "homepage": "http://www.codefocus.ca/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "illuminate/support": ">=5.4.0",
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Codefocus\\ManagedCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codefocus\\ManagedCache\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
